      "name": "üîê AUTHENTICATION - START HERE",
      "description": "Login with different user roles to test role-based access control",
      "item": [
        {
          "name": "üéØ Login as Director (Full Access)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "description": "Request body format"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"{{director_email}}\",\n  \"password\": \"{{director_password}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": ["{{base_url}}"],
              "path": ["auth", "login"]
            },
            "description": "**Director Role**: Full system access\n- View all teams, projects, reports\n- Manage mentors and announcements\n- Access comprehensive analytics\n\n**Test Account**: director@university.edu / director123"
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Director login successful\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.success).to.eql(true);",
                  "    pm.expect(jsonData.user.role).to.eql('director');",
                  "    pm.expect(jsonData.token).to.be.a('string');",
                  "});",
                  "",
                  "// Auto-save token for subsequent requests",
                  "var jsonData = pm.response.json();",
                  "pm.collectionVariables.set('jwt_token', jsonData.token);",
                  "pm.collectionVariables.set('current_user', JSON.stringify(jsonData.user));",
                  "",
                  "pm.test(\"Token saved for future requests\", function () {",
                  "    pm.expect(pm.collectionVariables.get('jwt_token')).to.be.a('string');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
